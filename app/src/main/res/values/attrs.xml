<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="customTitle2">
        <attr name="backText" format="string"/>
        <attr name="backTextColor" format="color"/>
        <attr name="backBg" format="reference"/>
        <attr name="rightText" format="string"/>
        <attr name="rightTextColor" format="color"/>
        <attr name="rightBg" format="reference"/>
        <attr name="titleText" format="string"/>
        <attr name="titleTexColor" format="color"/>
        <attr name="titleBg" format="reference"/>
    </declare-styleable>
    
    
    <declare-styleable name="passwordView">
        <attr name="borderWidth" format="dimension"/>
        <attr name="borderColor" format="color"/>
        <attr name="borderRadius" format="dimension"/>
        <attr name="passwordLength" format="integer"/>
        <attr name="passwordColor" format="color"/>
        <attr name="passwordRadius" format="dimension"/>
        <attr name="passwordwidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomRatingBar">
        <attr name="startNum" format="integer"/>
        <attr name="fillNum" format="integer"/>
        <attr name="isClickable" format="boolean"/>
        <attr name="fillDrawable" format="reference"/>
        <attr name="emptyDrawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CustomRatingBar2">
        <attr name="startCount" format="integer"/>
        <attr name="fillCount" format="float"/>
        <attr name="stepNum" format="float"/>
        <attr name="fillBitmap" format="reference"/>
        <attr name="emptyBitmap" format="reference"/>
        <attr name="betweenStar" format="dimension"/>
    </declare-styleable>

</resources>